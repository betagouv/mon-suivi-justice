<div class='fr-container static-wrapper fr-pb-6w fr-pt-8w'>
  <h1 class='static-title preparer-title fr-mb-4w'><%= t('preparer.spip92.title') %></h1>

  <div class='preparer-menu-trigger'>
    <%= t('preparer.spip92.question-place') %>
  </div>

  <div class='preparer-place-content static-content-container'>
    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.place-text1') %></p>

    <%= link_to 'https://goo.gl/maps/ejsmD6nAmqEfWAf4A', target: '_blank', class: 'static-google-map-link' do %>
      <div class='static-google-map-link-button'>
        <%= t('preparer.place-google') %>
      </div>
    <% end %>

    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.place-text2') %></p>
    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.place-text3') %></p>

    <h3 class='static-tertiary-title fr-mb-2w'><%= t('preparer.place-train') %></h3>

    <ol class='static-list'>
      <li class='static-list-item'><%= t('preparer.spip92.place-train1') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.place-train2') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.place-train3') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.place-train4') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.place-train5') %></li>
    </ol>

    <h3 class='static-tertiary-title fr-mb-2w'><%= t('preparer.place-bus') %></h3>

    <ul class='static-list'>
      <li class='static-list-item'><%= t('preparer.spip92.place-bus1') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.place-bus2') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.place-bus3') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.place-bus4') %></li>
    </ul>

    <h3 class='static-tertiary-title fr-mb-2w'><%= t('preparer.place-car') %></h3>
    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.place-car') %></p>
  </div>

  <div class='preparer-menu-trigger'>
    <%= t('preparer.question-no-show') %>
  </div>

  <div class='preparer-absence-content static-content-container'>
    <p class='static-text fr-mb-2w'><%= t('preparer.absence-text1') %></p>
    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.absence-text') %></p>
  </div>

  <div class='preparer-menu-trigger'>
    <%= t('preparer.question-justif') %>
  </div>

  <div class='preparer-general-content static-content-container'>
    <p class='static-text fr-mb-2w'><%= t('preparer.justificatifs-text1') %></p>
    <p class='static-text fr-mb-2w'><%= t('preparer.justificatifs-text2') %></p>
    <p class='static-text fr-mb-2w'><%= t('preparer.justificatifs-text3') %></p>

    <ul class='static-list'>
      <li class='static-list-item'><%= t('preparer.justificatifs-type1.html') %></li>
      <li class='static-list-item'><%= t('preparer.justificatifs-type2.html') %></li>
      <li class='static-list-item'><%= t('preparer.justificatifs-type3.html') %></li>
      <li class='static-list-item'><%= t('preparer.justificatifs-type4.html') %></li>
    </ul>

    <p class='static-text fr-mb-2w'><%= t('preparer.justificatifs-bonne-pratique') %></p>
  </div>

  <div class='preparer-menu-trigger'>
    <%= t('preparer.question-process') %>
  </div>

  <div class='preparer-head-content static-content-container preparer-head-text'>
    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.head-text1') %></p>

    <h2 class='static-secondary-title fr-mb-2w'><%= t('preparer.spip92.head-title1') %></h2>

    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.head-text2') %></p>
    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.head-text3') %></p>

    <ul class='static-list'>
      <li class='static-list-item'><%= t('preparer.spip92.head-text4') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.head-text5.html') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.head-text6.html') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.head-text7') %></li>
    </ul>

    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.head-text8') %></p>
    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.head-text9') %></p>

    <ul class='static-list'>
      <li class='static-list-item'><%= t('preparer.spip92.head-text10') %></li>
      <li class='static-list-item'><%= t('preparer.spip92.head-text11') %></li>
    </ul>

    <h2 class='static-secondary-title fr-mb-2w'><%= t('preparer.spip92.head-text12') %></h2>
    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.head-text13') %></p>
    <p class='static-text fr-mb-2w'><%= t('preparer.spip92.head-text14') %></p>
  </div>
</div>
