            <div class='stat-card-container'>
              <p class='stat-card-percentage'><%= t('stats.that_is') %> <b><%= t('stats.fulfiled_percentage', data: spip_data[:fulfiled_percentage]) %></b></p>
            </div>

            <div class='stat-card-container'>
              <p class='stat-card-title'><%= t('stats.excused') %></p>
              <p class='stat-card-value'><%= spip_data[:excused] %></p>
            </div>

            <div class='stat-card-container'>
              <p class='stat-card-percentage'><%= t('stats.that_is') %> <b><%= t('stats.excused_percentage', data: spip_data[:excused_percentage]) %></b></p>
            </div>

            <div class='stat-card-container'>
              <p class='stat-card-title'><%= t('stats.no_show') %></p>
              <p class='stat-card-value'><%= spip_data[:no_show] %></p>
            </div>

            <div class='stat-card-container'>
              <p class='stat-card-percentage'><%= t('stats.that_is') %> <b><%= t('stats.no_show_percentage', data: spip_data[:no_show_percentage]) %></b></p>
            </div>
          </div>
          </div>
          <div class='stat-card-container'>
            <p class='stat-card-title'><%= t('stats.uninformed') %></p>
